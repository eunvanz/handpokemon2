{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [{
      "source": "**/o/**",
      "headers": [{
        "key": "Cache-Control",
        "value": "max-age=1296000"
      }]
    }]
  }
}
